<Window x:Class="UP321.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UP321"
        mc:Ignorable="d"
        Title="MainWindow" Height="550" Width="800">
    <Grid Background="#FF2893FF">
        <Grid.RowDefinitions>
            <RowDefinition Height="37*"/>
            <RowDefinition Height="180*"/>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" x:Name="BackButt" Click="BackButt_Click">Назад</Button>
        <Frame NavigationUIVisibility="Hidden" x:Name="MainFrame" Grid.Row="1"/>
        <StackPanel Grid.Row="2" HorizontalAlignment="Right">
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Top">
                <Button x:Name="createQrBtn" Content="Создать QR" Width="150" Height="50" Click="Button_Click"></Button>
            </StackPanel>
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                <Image x:Name="imageQr" Height="200" Width="200" ></Image>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
